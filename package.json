{
  "name": "react-logic-switch",
  "version": "1.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build-examples": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "test:once": "CI=true npm run test",
    "eject": "react-scripts eject",
    "build": "NODE_ENV=production babel --presets minify --presets @babel/env src/lib --out-dir dist --ignore __tests__,spec.js,test.js,__snapshots__",
    "lint": "eslint src/**/*.js",
    "prettycheck": "prettier --check src/**/*.js",
    "prettify": "prettier --write src/**/*.js"
  },
  "pre-commit": [
    "prettycheck",
    "lint",
    "test:once"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "babel-preset-minify": "^0.5.1",
    "babel-preset-react-app": "^9.0.2",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-babel": "^5.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.2.0",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "prop-types": ">=15.5.7",
    "react": ">=15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elsevierlabs-os/react-logic-switch.git"
  }
}
